<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.resteassistesprevenu" android:versionCode="5"
	android:versionName="1.1.0">
	<uses-sdk android:minSdkVersion="7" />
	<uses-permission android:name="android.permission.INTERNET" />
	<application android:icon="@drawable/ic_logo" android:label="@string/app_name"
		android:debuggable="false">
		<activity android:name=".activities.IncidentsEnCoursActivity"
			android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".activities.NewIncidentActivity"
			android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".activities.FavorisActivity"
			android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<service
			android:name=".services.IncidentsTransportsBackgroundService">
			<intent-filter>
				<action android:name=".IncidentsBackgroundService.ACTION" />
			</intent-filter>
		</service>
		<provider android:name=".provider.DefaultContentProvider"
		android:authorities="com.resteassistesprevenu.provider" />
	</application>
</manifest> 
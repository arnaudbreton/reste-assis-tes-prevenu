<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/incidentEnCoursItemView" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:orientation="vertical">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal">
		<ImageView android:id="@+id/imgTypeLigne" android:layout_width="30px"
			android:layout_height="30px" />
		<ImageView android:id="@+id/imgNumLigne" android:layout_width="30px"
			android:layout_height="30px"  />
		<TextView xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:text=" " 
			android:textColor="#000000" />
		<TextView android:id="@+id/txtHeureIncident"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:paddingRight="3dip" 
			android:textColor="#000000"/>
		<ImageView android:layout_width="20px"
			android:layout_height="20px" android:src="@drawable/ic_vote_plus" />
		<TextView android:id="@+id/txtNbVotePlus"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:padding="3dip" 
			android:textColor="#000000"/>
		<ImageView android:layout_width="20px"
			android:layout_height="20px" android:src="@drawable/ic_vote_minus" />
		<TextView android:id="@+id/txtNbVoteMinus"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:padding="3dip" 
			android:textColor="#000000"/>
		<ImageView android:layout_width="20px"
			android:layout_height="20px" android:src="@drawable/ic_vote_end" />
		<TextView android:id="@+id/txtNbVoteEnd"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:padding="3dip" 
			android:textColor="#000000"/>
	</LinearLayout>
	<TextView xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/txtIncidentItemViewRaison" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textColor="#000000" />
</LinearLayout>